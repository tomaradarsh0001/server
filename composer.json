{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "barryvdh/laravel-dompdf": "^2.2",
    "botman/botman": "^2.8",
    "botman/driver-amazon-alexa": "^1.3",
    "botman/driver-facebook": "^1.11",
    "botman/driver-slack": "^1.0",
    "botman/driver-web": "^1.5",
    "doctrine/dbal": "^3.8",
    "guzzlehttp/guzzle": "^7.8",
    "kreait/firebase-php": "^7.16",
    "laravel/framework": "^10.0",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "maatwebsite/excel": "^3.1",
    "mews/captcha": "3.3",
    "mpdf/mpdf": "^8.2",
    "openspout/openspout": "4.28",
    "rap2hpoutre/fast-excel": "^5.5",
    "simplesoftwareio/simple-qrcode": "^4.2",
    "spatie/laravel-ignition": "^2.8",
    "spatie/laravel-permission": "^6.7",
    "twilio/sdk": "^8.2",
    "yajra/laravel-datatables": "10.0",
    "yajra/laravel-datatables-oracle": "^10.11"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/breeze": "^1.29",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app\\Helpers\\commonFunctions.php",
      "app\\Helpers\\UserActionLogHelper.php",
      "app\\Helpers\\GeneralFunctions.php",
      "app\\Helpers\\BreadcrumbsHelper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "10.x-dev"
    },
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
